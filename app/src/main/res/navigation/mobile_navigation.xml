<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.uzi.quiz.ui.try.TryFragment"
        android:label="@string/title_try"
        tools:layout="@layout/fragment_try" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.uzi.quiz.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.uzi.quiz.ui.exit.ExitFragment"
        android:label="@string/title_exit"
        tools:layout="@layout/fragment_exit" />
</navigation>
